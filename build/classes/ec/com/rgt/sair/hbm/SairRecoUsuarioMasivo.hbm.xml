<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ec.com.rgt.sair.hbm.SairRecoUsuarioMasivo" table="SAIR_RECO_USUARIO_MASIVO" schema="USRSAIR">
        <id name="idRecoUm" type="big_decimal">
            <column name="ID_RECO_UM" precision="22" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="idRecomendacion" type="big_decimal">
            <column name="ID_RECOMENDACION" precision="22" scale="0" />
        </property>
        <property name="idUsuario" type="string">
            <column name="ID_USUARIO" length="20" />
        </property>
    </class>
</hibernate-mapping>
